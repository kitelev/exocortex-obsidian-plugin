{
  "name": "E2E Test Vault",
  "version": "1.0.0",
  "description": "Test vault for Exocortex E2E testing",
  "plugins": {
    "exocortex-obsidian-plugin": {
      "enabled": true,
      "settings": {
        "ontologyPath": "ontologies",
        "enableUniversalLayout": true,
        "enableDynamicLayout": true,
        "debug": true
      }
    }
  },
  "structure": {
    "classes": [
      "Asset.md",
      "Task.md", 
      "Project.md",
      "Area.md",
      "Resource.md",
      "Person.md",
      "Meeting.md"
    ],
    "properties": [
      "name.md",
      "description.md",
      "status.md",
      "priority.md",
      "due_date.md",
      "category.md",
      "tags.md"
    ],
    "ontologies": [
      "core.ttl",
      "task-management.ttl",
      "assets.ttl"
    ],
    "test-assets": [
      "Test Asset 1.md",
      "Test Task 1.md",
      "Test Project 1.md"
    ]
  }
}